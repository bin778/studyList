    "def product(x):\n",
    "    ret = 1\n",
    "    for x_i in x:\n",
    "        ret *= x_i\n",
    "    \n",
    "    return ret\n",
    "\n",
    "x = [i + 1 for i in range(5)]\n",
    "\n",
    "print(product(x))"
   ]
