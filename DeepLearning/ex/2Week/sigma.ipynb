    "def sigma(x):\n",
    "    ret = 0\n",
    "    for x_i in x:\n",
    "        ret += x_i\n",
    "    \n",
    "    return ret\n",
    "\n",
    "x = [i + 1 for i in range(10)]\n",
    "\n",
    "print(sigma(x))"
   ]
